var sessionStorageValue = null;
var closedText = null;

function setCurrentLocation () {
  if (locations) {
    sessionStorageValue = JSON.parse(sessionStorage.getItem("selectedLocation"));
    var curentIndex = typeof sessionStorageValue === 'number' ? sessionStorageValue : 0;
    sessionStorage.setItem('selectedLocation', curentIndex);
    closedText = closedText;
  }
  return;
}
setCurrentLocation();

function updateLocation (index) {
  sessionStorageValue = JSON.parse(sessionStorage.getItem("selectedLocation"));
  var curentIndex = typeof sessionStorageValue === 'number' && index !== sessionStorageValue ? index : sessionStorageValue;
  sessionStorage.setItem('selectedLocation', curentIndex);
  closedText = closedText;
  return;
}

function setIndex(index = 0) {
  var curentIndex = typeof sessionStorageValue === 'number' && index !== sessionStorageValue ? index : sessionStorageValue;
  if (locations && locations[curentIndex]) {
    var selectedInformation = document.getElementById('selectedInformation');
    var dropdownMenuButton = document.getElementById('dropdownMenuButton');
    if (selectedInformation, dropdownMenuButton) {
      selectedInformation.innerHTML = locations[curentIndex].phone;
      dropdownMenuButton.innerHTML = locations[curentIndex].locationName;
    }
  }
  return;
}
if (locations) {
  document.addEventListener("DOMContentLoaded", function() {
    setIndex(sessionStorageValue);
  });
}</script><style>@media only screen and (min-width:0px) {
