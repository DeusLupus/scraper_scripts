  if (conditionfn()) {
    callback()
  } else {
    setTimeout(function () {
      waitCondition(conditionfn, callback, pollInterval)
    }, pollInterval || 500)
  }
}

function loadStyleSheet(src){
  if (document.createStyleSheet) document.createStyleSheet(src);
  else {
    var stylesheet = document.createElement('link');
    stylesheet.href = src;
    stylesheet.rel = 'stylesheet';
    stylesheet.type = 'text/css';
    document.getElementsByTagName('head')[0].appendChild(stylesheet);
  }
}</script><footer><div id="SiteFooter"><div class="d-none d-md-flex text-center text-md-left footer-contact text-white"><div class="container"><div class="row justify-content-between"><div class="col-md-6 col-lg-4"><h3 class="footer-title danger-underline text-left">Links</h3><div class="row"><div class="col-md-6 py-1 pr-0"><a class="footer-text" href="/">Home</a></div><div class="col-md-6 py-1 pr-0"><a class="footer-text" href="/about-autos-4-less">About us</a></div><div class="col-md-6 py-1 pr-0"><a class="footer-text" href="/inventory">Find a car</a></div><div class="col-md-6 py-1 pr-0"><a class="footer-text" href="/privacy-policy">Privacy policy</a></div><div class="col-md-6 py-1 pr-0"><a class="footer-text" href="/sell-my-car">Sell your car</a></div><div class="col-md-6 py-1 pr-0"><a class="footer-text" href="/terms-and-conditions">Terms &amp; conditions</a></div><div class="col-md-6 py-1 pr-0"><a class="footer-text" href="/car-loan-calculator">Car payment calculator</a></div><div class="col-md-6 py-1 pr-0"><a class="footer-text" href="/sitemap">Sitemap</a></div><div class="col-md-6 py-1 pr-0"><a class="footer-text" href="/car-loan">Get pre-approved</a></div><div class="col-md-6 py-1 pr-0"><a class="notranslate footer-text" id="footerLanguageLabel" href="javascript:void(0)" onClick="changeLanguage()">En Español</a></div></div></div><div class="col-md-3"> <h3 class="footer-title danger-underline text-left">Contacts</h3><div class="row py-1 pl-3"><div class="col-md-1 px-0"><i class="fi-phone footer-color-icon"></i></div><div class="col-md-10 pl-0"><span class="footer-text">253-840-5167</span></div></div><div class="row py-1 pl-3"><div class="col-md-12"><div class="row"><div class="col-md-1 px-0"><i class="fi-location footer-color-icon"></i></div><div class="col-md-10 pl-0"><a class="footer-text ml-0" href="https://g.page/autos4less?share" target="_blank" onclick="gtag_exec('drive to dealership', 'click')">1632 River Rd Puyallup, WA 98371</a></div></div></div></di