const dealerid = "5f5ba627893d520072121598"
const apiUrl = "https://api-v3-prod.321ignition.info"
const inventoryLocation = ""
//- Variables content from config.pug dealer data
const compareTools = false
const urlGetTradeInValue = "/trade-in-value"
var MOBILE_BREAKPOINT = 980

const timeSelectComponentHeader = getCurrentLocation()

if(customInlineStyles) {
  const urlPath = window.location.pathname.split('/')
  applyCustomStyles(customInlineStyles, urlPath[1])
}

const dateSelectComponentRef = dateSelectComponent(timeSelectComponentHeader)
dateSelectComponentRef.getRelativeHours()
window.popups = new Popups()
window.popups.initPopups()
var MICROPHONE_ENABLED_CLASS = 'icon-color-header'
var MICROPHONE_DISABLED_CLASS = 'icon-color-light'

addLoadEvent(function () {
  var widgetsContainer=document.createElement('div')
  widgetsContainer.className="widgets-container"
  if (!compareTools) {
    compareCarsTab(widgetsContainer)
    //- accessibilityButtons(widgetsContainer);
  }
  document.body.appendChild(widgetsContainer)
})

waitCondition (function () {
  return window.currentLanguage && window.hasOwnProperty('_speechSupported')
},
function () {
  if (window._speechSupported) {
    $('.fi-mic').removeClass(MICROPHONE_DISABLED_CLASS)
    $('.fi-mic').addClass(MICROPHONE_ENABLED_CLASS)
  }
})

waitCondition(function () { return 'ImgHomenet' in window }, function () {
  $('.img-homenet').each(function () {
    new ImgHomenet(this) // ImgHomenet performs a check at init time so elements won't be wrapped more than once
  })
})

function getCurrentLocation () {
  /** Oppening hours, multiple locations and inventory location
    to determine current location and hours available for dealer */
  //- headerOpeningHours and multipleLocations comes from header pug file
  const headerOpeningHours = header  && header.openingHours ? header.openingHours : ''
  const multipleLocations = header && header.locations ? header.locations : ''
  let selectedLocation = {}

  if (headerOpeningHours) {
    return headerOpeningHours
  }

  if(inventoryLocation) {
    selectedLocation = multipleLocations ? multipleLocations.find(location => location.locationName === inventoryLocation) : ''
  } else { // I need to check the content on session storage.
    try {
      const selectedIndex = sessionStorage.getItem('selectedLocation') || 0
      selectedLocation = multipleLocations[selectedIndex]
    } catch(err) {
      console.log('sessionStorage not available!', err)
    }
  }

  if (selectedLocation && Object.keys(selectedLocation).length) {
    dealerTimezone = selectedLocation.timezone
    return selectedLocation.openingHours
  }

  return {}
}</script><script type="application/ld+json">{"@context":"https://schema.org","@type":"WebSite","url":"https://autos4less.com","potentialAction":{"@type":"SearchAction","target":"https://autos4less.com/inventory?keyword={q}","query-input":"required name=q"},"description":"Visiting autos4less.com, Not Sure Where to Look? Click to Search Inventory to Find Your Next Car, Sell Your Car, Car-loan Calculator, Get Pre-approved, &amp; More.","keywords":"Auto Dealer, Car Delearship, Buy Used Car, Car Dealership in WA 98371"}</script></footer><script type="text/javascript" src="/ssr-theme/layout/components/img-homenet.cda39.js"></script></body></html>