  var activeLanguage = document.querySelector('#activeLanguageButton');

  function switchToEs () {
    activeLanguage.classList.remove('active-left');
    activeLanguage.classList.add('active-right');
  }

  function switchToEng () {
    activeLanguage.classList.remove('active-right');
    activeLanguage.classList.add('active-left');
  }

  $('#mobileSpanishButton').click(() => {
    switchToEs();
  });

  $('#mobileEnglishButton').click(() => {
    switchToEng();
  });
}
